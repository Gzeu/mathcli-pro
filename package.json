    "catalog.js",
    "README.md",
    "LICENSE",
    "HIGH_POTENTIAL_FEATURES.md"
  ],
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/Gzeu"
  },
  "os": [
    "darwin",
    "linux",
    "win32"
  ]
}
