{
  "name": "mathcli-pro",
  "version": "4.2.0",
  "description": "Modern CLI calculator for developers, engineers, and power users. Features AI API costs, MEV calculations, serverless optimization, advanced math, unit conversions, and more.",
  "main": "index.js",
  "bin": {
    "mathcli-pro": "bin/mathcli-pro"
  },
  "type": "module",
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=8.0.0"
  },
  "scripts": {
    "start": "node index.js",
    "test": "node --experimental-vm-modules ./node_modules/jest/bin/jest.js --passWithNoTests",
    "test:coverage": "node --experimental-vm-modules ./node_modules/jest/bin/jest.js --coverage --passWithNoTests",
    "test:watch": "node --experimental-vm-modules ./node_modules/jest/bin/jest.js --watch",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"**/*.{js,json,md}\"",
    "format:check": "prettier --check \"**/*.{js,json,md}\"",
    "clean": "rm -rf node_modules package-lock.json",
    "reinstall": "npm run clean && npm install",
    "prepare": "node -e \"const fs = require('fs'); const path = require('path'); if (!fs.existsSync('data')) fs.mkdirSync('data', { recursive: true });\""
  },
  "keywords": [
    "cli",
    "calculator",
    "math",
    "mathematics",
    "terminal",
    "command-line",
    "ai",
    "ai-cost-calculator",
    "openai",
    "claude",
    "gemini",
    "gpt-5",
    "prompt-caching",
    "batch-api",
    "mev",
    "mev-calculator",
    "defi",
    "arbitrage",
    "flashloan",
    "liquidation",
    "ethereum",
    "multiversx",
    "egld",
    "serverless",
    "serverless-cost",
    "aws-lambda",
    "cloudflare-workers",
    "vercel",
    "cost-optimizer",
    "conversion",
    "unit-conversion",
    "currency",
    "cryptocurrency",
    "chart",
    "visualization",
    "ascii-chart",
    "optimization",
    "batch-processing",
    "csv",
    "json",
    "nodejs",
    "developer-tools",
    "engineering",
    "scientific-calculator",
    "matrix",
    "physics",
    "probability",
    "gaming",
    "web3",
    "blockchain"
  ],
  "author": {
    "name": "George Pricop",
    "email": "pricopgeorge@gmail.com",
    "url": "https://github.com/Gzeu"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Gzeu/mathcli-pro.git"
  },
  "bugs": {
    "url": "https://github.com/Gzeu/mathcli-pro/issues"
  },
  "homepage": "https://github.com/Gzeu/mathcli-pro#readme",
  "license": "MIT",
  "dependencies": {
    "chalk": "^5.6.2",
    "cli-chart": "^0.3.1",
    "convert-units": "^2.3.4",
    "csv-parse": "^6.1.0",
    "figlet": "^1.9.3",
    "inquirer": "^13.1.0",
    "mathjs": "^15.0.0",
    "node-fetch": "^3.3.2",
    "rxjs": "^7.8.2"
  },
  "devDependencies": {
    "eslint": "^9.18.0",
    "prettier": "^3.4.2",
    "jest": "^30.2.0"
  },
  "files": [
    "bin/",
